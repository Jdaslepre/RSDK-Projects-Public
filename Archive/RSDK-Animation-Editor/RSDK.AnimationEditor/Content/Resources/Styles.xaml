                                        <Setter Target="ContentControlColumnDefinition.Width"
                                                Value="Auto" />
                                        <Setter Target="PrimaryItemsControlColumnDefinition.Width"
                                                Value="*" />
                                    </VisualState.Setters>
                                </VisualState>
